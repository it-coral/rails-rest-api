                    }
                  }
                }
              },
              "required": [
                "user",
                "meta"
              ]
            },
            "examples": {
              "application/json": {
                "user": {
                  "email": "mozell_bins@townepouros.org",
                  "first_name": null,
                  "last_name": null,
                  "country": null,
                  "state": null
                },
                "meta": {
                  "token": null
                }
              }
            }
          },
          "400": {
            "description": "registration is fail with existing email",
            "schema": {
              "type": "object",
              "properties": {
                "errors": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "items": {
                      "email": "array"
                    }
                  }
                }
              },
              "required": [
                "errors"
              ]
            },
            "examples": {
