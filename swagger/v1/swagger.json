    "/api/v1/groups": {
      "get": {
        "summary": "list of groups",
        "tags": [
          "Groups"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "type": "string",
            "required": true
          },
          {
            "name": "page",
            "in": "query",
            "type": "integer",
            "required": false
          },
          {
            "name": "count",
            "in": "query",
            "type": "integer",
            "required": false
          },
          {
            "name": "visibility",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "Filter groups, possible values: private, public"
          },
          {
            "name": "status",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "Filter groups, possible values: private, publicactive, pause"
          },
          {
            "name": "my",
            "in": "query",
            "type": "any",
            "required": false,
            "description": "return user's groups"
          }
        ],
        "responses": {
          "200": {
            "description": "returns",
            "schema": {
              "type": "object",
              "properties": {
                "groups": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "count_participants": {
                        "type": "integer",
                        "x-nullable": true
                      },
                      "created_at": {
                        "type": "datetime"
                      },
                      "description": {
                        "type": "text",
                        "x-nullable": true
                      },
                      "id": {
                        "type": "integer"
                      },
                      "organization_id": {
                        "type": "integer",
                        "x-nullable": true
                      },
                      "status": {
                        "type": "string",
                        "x-nullable": true,
                        "enum": [
                          "active",
                          "pause"
                        ]
                      },
                      "title": {
                        "type": "string",
                        "x-nullable": true
                      },
                      "updated_at": {
                        "type": "datetime"
                      },
                      "user_id": {
                        "type": "integer",
                        "x-nullable": true
                      },
                      "user_limit": {
                        "type": "integer",
                        "x-nullable": true
                      },
                      "visibility": {
                        "type": "string",
                        "x-nullable": true,
                        "enum": [
                          "private",
                          "public"
                        ]
                      }
                    }
                  }
                }
              },
              "required": [
                "groups"
              ]
            },
            "examples": {
              "application/json": {
                "groups": [
                  {
                    "count_participants": null,
                    "created_at": "2018-02-21T19:02:11.610Z",
                    "description": "extend clicks-and-mortar action-items",
                    "id": 1,
                    "organization_id": 1,
                    "status": "active",
                    "title": "Lebsack Inc",
                    "updated_at": "2018-02-21T19:02:11.610Z",
                    "user_id": null,
                    "user_limit": 10,
                    "visibility": "public",
                    "organization": {
                    }
                  }
                ],
                "meta": {
                  "total_pages": 1,
                  "size": 1,
                  "limit_value": 20,
                  "current_page": 1
                }
